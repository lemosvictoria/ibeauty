<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.iesb.mobile.ibeauty">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar">
        <activity android:name=".ui.fragment.OnboardingFragment"></activity>
        <activity android:name=".ui.activity.CadastroCliente2" />
        <activity android:name=".ui.activity.CadastroCliente1" />
        <activity android:name=".ui.activity.LoginActivity" />
        <activity android:name=".ui.activity.TipoCadastro" />
        <activity android:name=".ui.activity.CadastroProfissional2" />
        <activity android:name=".ui.activity.CadastroProfissional1" />
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>